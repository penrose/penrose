{
  "name": "@penrose/core",
  "version": "3.0.0-alpha.0",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Penrose Team (https://penrose.cs.cmu.edu/)",
  "license": "MIT",
  "files": [
    "dist/**/*.d.ts",
    "dist/**/*.js"
  ],
  "scripts": {
    "build-parsers": "nearleyc src/parser/Domain.ne > src/parser/DomainParser.ts && nearleyc src/parser/Substance.ne > src/parser/SubstanceParser.ts && nearleyc src/parser/Style.ne > src/parser/StyleParser.ts",
    "build": "tsc",
    "watch": "yarn build --watch",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "build-decls": ":",
    "typecheck": ":",
    "docs": "typedoc --out docs",
    "docs:md": "typedoc --plugin typedoc-plugin-markdown --out docs/md",
    "lint": "eslint --ext js,ts,tsx src"
  },
  "nx": {
    "targets": {
      "build": {
        "dependsOn": [
          "^build",
          "^build-decls",
          "build-parsers"
        ]
      },
      "build-decls": {
        "dependsOn": [
          "build"
        ]
      },
      "coverage": {
        "dependsOn": [
          "^build",
          "^build-decls",
          "build-parsers"
        ]
      },
      "docs": {
        "dependsOn": [
          "^build",
          "^build-decls",
          "build-parsers"
        ]
      },
      "test": {
        "dependsOn": [
          "^build",
          "^build-decls",
          "build-parsers"
        ]
      },
      "test-watch": {
        "dependsOn": [
          "^build",
          "^build-decls",
          "build-parsers"
        ]
      },
      "watch": {
        "dependsOn": [
          "^build",
          "build-parsers"
        ]
      }
    }
  },
  "dependencies": {
    "@datastructures-js/queue": "^4.1.3",
    "@penrose/optimizer": "^3.0.0-alpha.0",
    "consola": "^2.15.2",
    "immutable": "^4.0.0-rc.12",
    "lodash": "^4.17.15",
    "mathjax-full": "^3.0.1",
    "moo": "^0.5.1",
    "nearley": "^2.20.1",
    "pandemonium": "^2.0.0",
    "poly-partition": "^1.0.2",
    "recursive-diff": "^1.0.8",
    "seedrandom": "^3.0.5",
    "true-myth": "^4.1.0"
  },
  "devDependencies": {
    "@penrose/examples": "^3.0.0-alpha.0",
    "@types/lodash": "^4.14.149",
    "@types/moo": "^0.5.3",
    "@types/nearley": "^2.11.1",
    "@types/node": "^10.17.21",
    "@types/seedrandom": "^2.4.28",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.3",
    "tslib": "^2.1.0",
    "typedoc": "^0.24.7",
    "typedoc-plugin-markdown": "^3.11.13"
  }
}
