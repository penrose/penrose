    "watch": "NODE_OPTIONS='--max-old-space-size=8192' docusaurus start",
    "build": "shx mkdir -p ./static/try/;shx cp -r ../editor/dist/* ./static/try;cd ../automator;yarn start shapedefs ../docs-site/static/shapedefs.json;cd ../docs-site; NODE_OPTIONS='--max-old-space-size=8192' docusaurus build --no-minify",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "clean": "rimraf .docusaurus build"
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-beta.20",
    "@docusaurus/preset-classic": "2.0.0-beta.20",
    "@mdx-js/react": "^1.6.21",
    "@penrose/components": "^1.3.0",
    "@penrose/core": "^1.3.0",
    "@penrose/editor": "^1.3.0",
    "@penrose/examples": "^1.3.0",
    "clsx": "^1.1.1",
    "hast-util-is-element": "1.1.0",
    "prism-react-renderer": "^1.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rehype-katex": "5",
    "remark-math": "3",
    "seedrandom": "^3.0.5",
