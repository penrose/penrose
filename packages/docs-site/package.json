{
  "name": "@penrose/docs-site",
  "version": "3.0.0-alpha.1",
  "private": true,
  "scripts": {
    "build": "vitepress build",
    "build-decls": ":",
    "build-shapedefs": "shx mkdir -p ./public/try/ && shx cp -r ../editor/dist/* ./public/try && shx rm -f ./src/shapedefs.json && cd ../roger && yarn start shapedefs -o ../docs-site/src/shapedefs.json",
    "dev": "vitepress dev",
    "preview": "vitepress preview",
    "typecheck": ":"
  },
  "nx": {
    "targets": {
      "build": {
        "dependsOn": [
          "^build",
          "build-shapedefs"
        ]
      },
      "build-shapedefs": {
        "dependsOn": [
          "^build"
        ]
      },
      "dev": {
        "dependsOn": [
          "^build",
          "build-shapedefs"
        ]
      },
      "preview": {
        "dependsOn": [
          "build"
        ]
      }
    }
  },
  "dependencies": {
    "@penrose/components": "^3.0.0-alpha.0",
    "@penrose/examples": "^3.0.0-alpha.0",
    "veaury": "^2.3.11"
  },
  "devDependencies": {
    "@penrose/roger": "^3.0.0-alpha.1",
    "@penrose/editor": "^3.0.0-alpha.0",
    "markdown-it-katex": "^2.0.3",
    "shx": "^0.3.3",
    "vitepress": "^1.0.0-alpha.35"
  }
}
