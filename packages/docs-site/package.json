{
  "name": "@penrose/docs-site",
  "version": "1.3.0",
  "private": true,
  "scripts": {
    "build": "vitepress build",
    "build-decls": ":",
    "build-shapedefs": "shx mkdir -p ./public/try/ && shx cp -r ../editor/dist/* ./public/try && shx rm -f ./src/shapedefs.json && cd ../automator && yarn start shapedefs ../docs-site/src/shapedefs.json",
    "dev": "vitepress dev",
    "preview": "vitepress preview",
    "typecheck": ":"
  },
  "nx": {
    "targets": {
      "build": {
        "dependsOn": [
          "^build",
          "build-shapedefs"
        ],
        "outputs": [
          "{projectRoot}/build"
        ]
      },
      "build-shapedefs": {
        "dependsOn": [
          "^build"
        ],
        "outputs": [
          "{projectRoot}/public/try",
          "{projectRoot}/src/shapedefs.json"
        ]
      },
      "dev": {
        "dependsOn": [
          "^build",
          "build-shapedefs"
        ]
      },
      "preview": {
        "dependsOn": [
          "build"
        ]
      }
    }
  },
  "devDependencies": {
    "@penrose/automator": "1.3.0",
    "@penrose/editor": "1.3.0",
    "markdown-it-katex": "^2.0.3",
    "shx": "^0.3.3",
    "vitepress": "^1.0.0-alpha.33"
  }
}
