{
  "name": "@penrose/docs-site",
  "version": "3.0.0-alpha.1",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "vitepress build",
    "build-decls": ":",
    "copy-editor": "shx mkdir -p ./public/try/ && shx cp -r ../editor/dist/* ./public/try",
    "build-shapedefs": "shx rm -f ./src/shapedefs.json && roger shapedefs -o ./src/shapedefs.json",
    "dev": "vitepress dev",
    "preview": "vitepress preview",
    "typecheck": ":"
  },
  "nx": {
    "targets": {
      "copy-editor": {
        "dependsOn": [
          "^build"
        ]
      },
      "build": {
        "dependsOn": [
          "^build",
          "copy-editor",
          "build-shapedefs"
        ]
      },
      "build-shapedefs": {
        "dependsOn": [
          "^build"
        ]
      },
      "dev": {
        "dependsOn": [
          "^build",
          "copy-editor",
          "build-shapedefs"
        ]
      },
      "preview": {
        "dependsOn": [
          "build"
        ]
      }
    }
  },
  "dependencies": {
    "@penrose/components": "^3.0.0-alpha.0",
    "@penrose/examples": "^3.0.0-alpha.0",
    "veaury": "^2.3.11"
  },
  "devDependencies": {
    "tailwindcss": "^3.1.8",
    "@tailwindcss/typography": "^0.5.4",
    "@penrose/core": "^3.0.0-alpha.0",
    "@penrose/editor": "^3.0.0-alpha.0",
    "@penrose/roger": "^3.0.0-alpha.1",
    "@types/markdown-it": "^12.2.3",
    "markdown-it": "^13.0.1",
    "markdown-it-katex": "^2.0.3",
    "shx": "^0.3.3",
    "vite-plugin-top-level-await": "^1.2.3",
    "vitepress": "^1.0.0-beta.1"
  }
}
