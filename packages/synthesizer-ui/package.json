{
  "name": "@penrose/synthesizer-ui",
  "private": true,
  "version": "1.3.0",
  "scripts": {
    "dev": "vite",
    "watch": "vite",
    "start": "vite",
    "typecheck": "tsc --noEmit",
    "build": "NODE_OPTIONS='--max-old-space-size=8192' vite build",
