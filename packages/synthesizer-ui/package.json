{
  "name": "@penrose/synthesizer-ui",
  "private": true,
  "version": "1.3.0",
  "scripts": {
    "dev": "vite",
    "watch": "vite",
    "start": "vite",
    "typecheck": "tsc --noEmit",
    "build-decls": ":",
    "build": "cross-env NODE_OPTIONS='--max-old-space-size=8192' vite build",
    "preview": "vite preview"
  },
  "nx": {
    "targets": {
      "build": {
        "dependsOn": [
          "^build"
        ]
      },
      "dev": {
        "dependsOn": [
          "^build"
        ]
      },
      "preview": {
        "dependsOn": [
          "^build"
        ]
      },
      "start": {
        "dependsOn": [
          "^build"
        ]
      },
      "watch": {
        "dependsOn": [
          "^build"
        ]
      }
    }
  },
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@penrose/components": "1.3.0",
    "@penrose/core": "1.3.0",
    "@penrose/examples": "1.3.0",
    "@types/file-saver": "^2.0.5",
    "@types/jszip": "^3.4.1",
    "file-saver": "^2.0.5",
    "jszip": "^3.10.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react-swc": "^3.0.1",
    "vite": "^4.0.4"
  }
}
