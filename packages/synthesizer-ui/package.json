{
  "name": "@penrose/synthesizer-ui",
  "private": true,
  "version": "1.3.0",
  "scripts": {
    "dev": "vite",
    "watch": "vite",
    "start": "vite",
    "typecheck": "tsc --noEmit",
    "build-decls": ":",
    "build": "cross-env NODE_OPTIONS='--max-old-space-size=8192' vite build",
    "preview": "vite preview"
  },
  "nx": {
    "targets": {
      "build": {
        "dependsOn": [
          "^build"
        ],
        "outputs": [
          "{projectRoot}/dist"
        ]
      },
      "dev": {
        "dependsOn": [
          "^build"
        ]
      },
      "preview": {
        "dependsOn": [
          "^build"
        ]
      },
      "start": {
        "dependsOn": [
          "^build"
        ]
      },
      "watch": {
        "dependsOn": [
          "^build"
        ]
      }
    }
  },
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@penrose/components": "1.3.0",
    "@penrose/core": "1.3.0",
    "@penrose/examples": "1.3.0",
    "@types/file-saver": "^2.0.5",
    "@types/jszip": "^3.4.1",
    "file-saver": "^2.0.5",
    "jszip": "^3.10.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@vitejs/plugin-react": "^1.0.7",
    "typescript": "^4.5.4",
    "vite": "^2.8.0"
  }
}
