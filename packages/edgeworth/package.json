{
  "name": "@penrose/edgeworth",
  "private": true,
  "version": "3.0.0-beta.1",
  "scripts": {
    "dev": "vite",
    "watch": "vite",
    "start": "vite",
    "typecheck": "tsc",
    "build-decls": ":",
    "build": "cross-env NODE_OPTIONS='--max-old-space-size=8192' vite build",
    "preview": "vite preview",
    "test": "vitest run --no-threads",
    "coverage": "yarn test --coverage"
  },
  "nx": {
    "targets": {
      "build": {
        "dependsOn": [
          "^build"
        ]
      },
      "dev": {
        "dependsOn": [
          "^build"
        ]
      },
      "preview": {
        "dependsOn": [
          "^build"
        ]
      },
      "start": {
        "dependsOn": [
          "^build"
        ]
      },
      "watch": {
        "dependsOn": [
          "^build"
        ]
      },
      "test": {
        "dependsOn": [
          "^build",
          "^build-decls"
        ]
      },
      "coverage": {
        "dependsOn": [
          "^build",
          "^build-decls"
        ]
      }
    }
  },
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@penrose/components": "^3.0.0-beta.1",
    "@penrose/core": "^3.0.0-beta.1",
    "@penrose/examples": "^3.0.0-beta.1",
    "@types/file-saver": "^2.0.5",
    "@types/jszip": "^3.4.1",
    "file-saver": "^2.0.5",
    "jszip": "^3.10.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-latex-next": "^2.2.0",
    "react-router-dom": "^6.9.0"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.5",
    "@types/jszip": "^3.4.1",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react-swc": "^3.0.1",
    "canvas": "^2.8.0",
    "vite": "^4.0.4",
    "vitest": "^0.31.1"
  }
}
