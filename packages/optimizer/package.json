{
  "name": "@penrose/optimizer",
  "version": "1.3.0",
  "private": true,
  "scripts": {
    "build": "esbuild ./source.js --bundle --loader:.wasm=binary --outfile=index.js --format=cjs",
    "build-wasm": "cargo build --release"
  },
  "nx": {
    "targets": {
      "build": {
        "dependsOn": [
          "build-wasm"
        ],
        "outputs": [
          "{projectRoot}/index.js"
        ]
      },
      "build-wasm": {
        "outputs": [
          "{projectRoot}/target"
        ]
      }
    }
  },
  "devDependencies": {
    "esbuild": "^0.15.7"
  }
}
