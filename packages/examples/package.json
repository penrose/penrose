{
  "name": "@penrose/examples",
  "version": "3.0.0-alpha.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@penrose/solids": "^3.0.0-alpha.0"
  },
  "scripts": {
    "build": "rimraf dist && tsc",
    "build-decls": ":",
    "codegen": "git clean -dfqX src && node codegen.js",
    "typecheck": ":"
  },
  "nx": {
    "targets": {
      "build": {
        "dependsOn": [
          "^build",
          "^build-decls",
          "codegen"
        ]
      },
      "build-decls": {
        "dependsOn": [
          "build"
        ]
      }
    }
  }
}
