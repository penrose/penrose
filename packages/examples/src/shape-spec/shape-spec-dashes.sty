canvas {
  width = 800
  height = 700
}

forall Path A {
    A.icon = Path {
        d : pathFromPoints("open", [(0,0), (0,100), (100,100)])
        strokeWidth : 5
        strokeDasharray : "5 5 20 2"
    }
}

forall Circle E {
    E.icon = Circle { 
        strokeColor : rgba(0.,0.,0.,1.)
        strokeWidth : 5
        --pathLength : 2
    }
}

forall Rectangle F {
    F.icon = Rectangle { 
        strokeColor : rgba(0.,0.,0.,1.)
        strokeDasharray : "30 30"
        strokeWidth : 5
    }
}

forall Line G {
    G.icon = Line {
        strokeDasharray : "5 5 20 2"
    }
}

forall Equation H {
    H.icon = Equation { }
}

forall Image I {
    I.icon = Image { }
}

forall DashedArrow J {
    J.icon = Line {
        style : "dashed"
        endArrowhead: "straight"
    }
}

forall DashArrayArrow K {
    K.icon = Line {
        strokeDasharray : "30 10 1 10"
        endArrowhead: "straight"
    }
}