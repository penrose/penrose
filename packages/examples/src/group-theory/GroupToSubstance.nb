                "\[RightDoubleBracket]"}]}], "]"}]}], "]"}]}], 
          "\[IndentingNewLine]", "}"}], "&"}], "/@", "elements"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"{", 
        RowBox[{"i", ",", "1", ",", 
         RowBox[{"Length", "[", "elements", "]"}]}], "}"}]}], 
      "\[IndentingNewLine]", "]"}]}], ";"}], "\[IndentingNewLine]", 
   "\[IndentingNewLine]", 
