-- electrons
Electron e1, e2, e3, e4
e1 := ValenceElectron(o1)
e2 := ValenceElectron(o1)
e3 := ValenceElectron(o1)
e4 := ValenceElectron(o1)
Electron e5, e6
e5 := ValenceElectron(n)
e6 := ValenceElectron(n)
Electron e7, e8, e9, e10
e7  := ValenceElectron(o2)
e8  := ValenceElectron(o2)
e9  := ValenceElectron(o2)
e10 := ValenceElectron(o2)