canvas {
  width = 800
  height = 700
}

Path A {
    A.icon = Path {
        pathData : pathFromPoints("open", [(0,0), (0,100), (100,100)])
        strokeWidth : 5
        strokeDashArray : "5 5 20 2"
    }
}

Arrow C {
    C.icon = Arrow { }
}

Square D {
    D.icon = Square { 
        strokeDashArray : "5 5 20 2"
        strokeColor : rgba(0.,0.,0.,1.)
        strokeWidth : 5
    }
}

Circle E {
    E.icon = Circle { 
        strokeColor : rgba(0.,0.,0.,1.)
        strokeWidth : 5
        pathLength : 2
    }
}

Rectangle F {
    F.icon = Rectangle { 
        strokeColor : rgba(0.,0.,0.,1.)
        strokeDashArray : "30 30"
        strokeWidth : 5
    }
}

Line G {
    G.icon = Line {
        strokeDashArray : "5 5 20 2"
    }
}

Equation H {
    H.icon = Equation { }
}

Image I {
    I.icon = Image { }
}

DashedArrow J {
    J.icon = Arrow {
        style : "dashed"
    }
}

DashArrayArrow K {
    K.icon = Arrow {
        strokeDashArray : "30 10 1 10"
    }
}