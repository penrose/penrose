forall Path a {
    a.icon = Path { }
}

forall Arrow c {
    c.icon = Arrow { }
}

forall Square d {
    d.icon = Square { 
        rx: 0
        rotation: 45
    }
}

forall Circle e {
    e.icon = Circle {
        center: [20,20]
        r: 40.0
        -- fill: rgba(255.0,43.0,0.0,0.0)
        -- pathLength: 6.0
     }
}

forall Rectangle f {
    f.icon = Rectangle { 
        rx: 20
        center: (20,20)
    }
}

forall Line g {
    g.icon = Line { }
}

forall Text h {
    h.icon = Text { 
        string: "i am a label"
        rotation: 45
    }
}

forall Image i {
    i.icon = Image { }
}

forall Polygon j {
	j.icon = Polygon { 
        points: ((-80.0, 0.0), (100.0, 0.0), (10.0, 50.0))
        strokeWidth: 20
        scale : 1
	}
}

forall Polyline k {
	k.icon = Polyline { 
        points: ((-20.0, 0.0), (100.0, 0.0), (10.0, 50.0))
        strokeWidth: 100
        scale : 2
        center : (2,3)
	}
}

forall Ellipse l {
    l.icon = Ellipse {
        center: [20,20]
        rx: 20
        ry: 20
     }
}

forall FreeformPolygon u {
	u.icon = FreeformPolygon { 
        points: ((-80.0, 0.0), (100.0, 0.0), (10.0, 50.0))
        -- strokeWidth: 20
        -- scale: 2
	}
}

forall PathString p {
    p.icon = PathString {
        w: 300
        h: 300
        data: "M 10 80 C 40 10, 65 10, 95 80 S 150 150, 180 80"
        viewBox: "0 0 200 200"
    }
}